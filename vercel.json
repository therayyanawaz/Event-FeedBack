{
  "buildCommand": "next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/*/route.ts": {
      "runtime": "@vercel/node@2.15.3"
    },
    "app/api/*/*/route.ts": {
      "runtime": "@vercel/node@2.15.3"
    },
    "app/api/*/*/*/route.ts": {
      "runtime": "@vercel/node@2.15.3"
    }
  }
} 